<% layout('/layouts/boilerplate') %>
    <h1>
        <%= member.handle %>
    </h1>
    <h1>
        <%= member.email %>
    </h1>
    <p>
        <a href="/members/<%= member._id %>/edit">Edit</a>
    </p>
    <form action="/members/<%= member._id %>?_method=DELETE" method="post">
        <button>Delete</button>
    </form>
    <footer>
        <a href="/members">Members</a>
    </footer>